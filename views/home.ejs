<%- include('includes/header.ejs') %>
  </head>

  <body>
    <%- include('includes/navigation.ejs') %>
      <% if(proje.length>0){ %>
        <% for (let project of proje) { %>
          <a href="#"> <h1><%= project.projectName %> </h1></a>
           <a href="/projects/<%= project.id %>" class="btn">Details</a>
            <p><%= project.description %>  </p>
            <p><%= project.authorName %> </p>
        <% } %>
      <% } else { %>
           <h1>No Project Found!</h1>
      <% } %>
  </body>

  </html>
